  "license": "MIT",
  "homepage": "https://github.com/thomas-coldwell/expo-image-editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomas-coldwell/expo-image-editor"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "expo-image-manipulator": "*",
    "expo-gl": "*",
    "expo-gl-cpp": "*",
    "expo-file-system": "*",
    "expo-asset": "*",
    "@expo/vector-icons": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@expo/webpack-config": "^0.12.11",
    "@expo/vector-icons": "^12.0.5",
    "@types/react": "~16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "~0.63.2",
    "babel-preset-expo": "~8.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-builder-bob": "^0.18.2",
    "react-native-web": "~0.13.12",
    "standard-version": "^9.2.0",
    "typescript": "^3.9.3",
    "@miblanchard/react-native-slider": "^1.5.0",
    "@react-hook/throttle": "^2.2.0",
    "@types/lodash": "^4.14.157",
    "expo-asset": "^8.1.7",
    "expo-file-system": "^9.2.0",
    "expo-gl": "^9.1.1",
    "expo-gl-cpp": "^9.1.2",
    "expo-image-manipulator": "^9.2.2",
    "modal-enhanced-react-native-web": "^0.2.0",
    "modal-react-native-web": "^0.2.0",
    "no-scroll": "^2.1.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-safe-area-context": "^3.2.0",
    "recoil": "^0.0.10"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  }
